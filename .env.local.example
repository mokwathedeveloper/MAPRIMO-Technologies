# Supabase Configuration
# Replace with your project details from Supabase Dashboard > Settings > API
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Google Integration (Optional)
# Required for Google Sheets sync and Calendar events
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REFRESH_TOKEN=your_google_refresh_token
GOOGLE_SHEET_ID=your_spreadsheet_id

# Analytics & Site Config
# Replace with your GA4 Measurement ID (e.g., G-XXXXXXXXXX)
NEXT_PUBLIC_GA_ID=
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Email (Resend) - Optional
# Required for automated lead notifications
RESEND_API_KEY=re_123456789
